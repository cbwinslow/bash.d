# Multi-Agentic System Environment Variables

# RabbitMQ
RABBITMQ_PASSWORD=secure_rabbitmq_password

# PostgreSQL
POSTGRES_PASSWORD=secure_postgres_password

# MinIO
MINIO_PASSWORD=secure_minio_password

# Grafana
GRAFANA_PASSWORD=secure_grafana_password

# AI Provider API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cloudflare (for deployment)
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id

# Supabase (optional)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Bitwarden CLI Configuration
# For secret management and secure credential storage
BW_EMAIL=your-bitwarden-email@example.com
BW_PASSWORD=your-bitwarden-master-password
BW_SESSION=  # Will be set automatically after unlock
# Alternative: Use API key for non-interactive login
BW_CLIENTID=your-bitwarden-client-id
BW_CLIENTSECRET=your-bitwarden-client-secret

# Application Settings
LOG_LEVEL=INFO
MAX_CONCURRENT_TASKS=100
HEALTH_CHECK_INTERVAL=30
ORCHESTRATION_STRATEGY=specialized
