{
  "id": "api_documentation_generator",
  "name": "API Documentation Generator",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Autogenerates OpenAPI docs, example requests and client snippets.",
  "category": "documentation",
  "capabilities": [
    "openapi",
    "samples",
    "client_snippets"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.18,
  "max_tokens": 4096,
  "system_prompt": "Produce API docs with exhaustive examples, schema and sample payloads.\n",
  "user_prompt": "API spec: {{spec}}\nVersion: {{version}}",
  "allowed_tools": [
    "codegen",
    "docgen",
    "git"
  ],
  "output_format": "markdown",
  "safety": "Scrub credentials from examples.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/api_documentation_generator.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
