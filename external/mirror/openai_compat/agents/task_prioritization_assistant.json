{
  "id": "task_prioritization_assistant",
  "name": "Task Prioritization Assistant",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Ranks backlog items with user-context and suggests scope for next sprint.",
  "category": "automation",
  "capabilities": [
    "prioritization",
    "estimation",
    "roadmapping"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.4,
  "max_tokens": 1024,
  "system_prompt": "Provide prioritized lists with rationale and risk impact to help sprint planning decisions.\n",
  "user_prompt": "Backlog: {{items}}\nGoals: {{goals}}",
  "allowed_tools": [
    "issue_tracker",
    "estimation_tools"
  ],
  "output_format": "markdown",
  "safety": "Avoid authoritative decisions that require stakeholder consensus.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/task_prioritization_assistant.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
