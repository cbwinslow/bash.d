{
  "id": "visual_designer",
  "name": "Visual Designer",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Produces design tokens, visual systems, and quick mockups for product features.",
  "category": "design",
  "capabilities": [
    "style_guides",
    "tokens",
    "mockups"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.35,
  "max_tokens": 2048,
  "system_prompt": "Provide accessible style guides and tokens that scale across components.\n",
  "user_prompt": "Brand details: {{brand}}\nUse-cases: {{usecases}}",
  "allowed_tools": [
    "design_tooling",
    "assets_repo"
  ],
  "output_format": "markdown",
  "safety": "Respect IP and licensed assets.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/visual_designer.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
