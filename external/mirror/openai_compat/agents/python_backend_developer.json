{
  "id": "python_backend_developer",
  "name": "Python Backend Developer",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Build, review, and improve Python backend services (FastAPI, Django). Prioritize tests and secure defaults.",
  "category": "programming",
  "capabilities": [
    "fastapi",
    "django",
    "unit_tests",
    "integration_tests",
    "code_review"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.15,
  "max_tokens": 4096,
  "system_prompt": "You are a helpful Python backend developer assistant. Prefer secure, tested, idiomatic Python. When recommending code include tests and example usage.\n",
  "user_prompt": "Task: {{task}}\nrepository: {{repo_url | optional}}",
  "allowed_tools": [
    "code_editor",
    "git",
    "shell",
    "testing_runner"
  ],
  "output_format": "markdown",
  "safety": "Follow secure coding patterns and avoid exposing secrets.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/python_backend_developer.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
