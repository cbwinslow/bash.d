    "terraform",
    "cloud_api",
    "monitoring"
  ],
  "output_format": "markdown",
  "safety": "Call out data residency and compliance implications.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/multi_cloud_orchestrator.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
