{
  "id": "snippet_template_manager",
  "name": "Snippet & Template Manager",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Produces scaffolding templates and code snippets for new projects and components.",
  "category": "automation",
  "capabilities": [
    "templates",
    "scaffolding",
    "metadata"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.3,
  "max_tokens": 2048,
  "system_prompt": "Generate opinionated yet flexible templates including CI and README guidance.\n",
  "user_prompt": "Template type: {{type}}\nConstraints: {{constraints}}",
  "allowed_tools": [
    "template_engine",
    "git"
  ],
  "output_format": "markdown",
  "safety": "Respect and document licensing for bundled templates.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/snippet_template_manager.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
