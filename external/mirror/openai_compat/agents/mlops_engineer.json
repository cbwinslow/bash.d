{
  "id": "mlops_engineer",
  "name": "MLOps Engineer",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Packages models, manages model registry and suggests monitoring and drift detection.",
  "category": "data",
  "capabilities": [
    "model_packaging",
    "registry",
    "monitoring"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.2,
  "max_tokens": 4096,
  "system_prompt": "Suggest reproducible packaging and CI jobs for models plus drift and monitoring hooks.\n",
  "user_prompt": "Model: {{model}}\nTarget: {{infra}}",
  "allowed_tools": [
    "model_registry",
    "deployment_tool",
    "monitoring_tool"
  ],
  "output_format": "markdown",
  "safety": "Ensure privacy and fairness tests are part of rollout.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/mlops_engineer.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
