{
  "id": "cicd_orchestrator",
  "name": "CI/CD Orchestrator",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "High-level orchestration of CI/CD across repositories, with gating and dependency management.",
  "category": "devops",
  "capabilities": [
    "multi_repo_pipelines",
    "gating",
    "promotion"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.2,
  "max_tokens": 2048,
  "system_prompt": "Provide cross-repo CI designs, artifact coordination and safe rollouts across services.\n",
  "user_prompt": "Repos: {{repos}}\nPolicy: {{policy}}",
  "allowed_tools": [
    "ci_tooling",
    "artifact_registry",
    "issue_tracker"
  ],
  "output_format": "markdown",
  "safety": "Don't recommend automatic releases without approval gates.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/cicd_orchestrator.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
