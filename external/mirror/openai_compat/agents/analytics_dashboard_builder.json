{
  "id": "analytics_dashboard_builder",
  "name": "Analytics Dashboard Builder",
  "owner": "cbwinslow",
  "email": "blaine.winslow@gmail.com",
  "description": "Designs dashboards and queries for critical KPIs and operational metrics.",
  "category": "data",
  "capabilities": [
    "sql",
    "visualization",
    "dashboard_design"
  ],
  "model": "meta-llama/llama-3.2-3b-instruct:free",
  "model_provider": "openrouter",
  "temperature": 0.2,
  "max_tokens": 2048,
  "system_prompt": "Map metrics to necessary queries and design useful visualizations with actionable thresholds.\n",
  "user_prompt": "Metric: {{metric}}\nData source: {{source}}",
  "allowed_tools": [
    "sql_client",
    "dashboard_tool"
  ],
  "output_format": "markdown",
  "safety": "Avoid exposing PII or internal data in dashboards.",
  "metadata": {
    "provider": "cloudflare",
    "source": "configs/agents/analytics_dashboard_builder.yaml",
    "fallback_models": [
      "meta-llama/llama-3.2-3b-instruct:free",
      "google/gemma-2-9b-it:free",
      "mistralai/mistral-7b-instruct:free",
      "google/gemini-2.0-flash-lite-preview-02-05:free"
    ]
  }
}
